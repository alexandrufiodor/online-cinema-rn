{
	"expo": {
		"name": "Online Cinema",
		"slug": "online-cinema",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#111111"
		},
		"ios": {
			"supportsTablet": true,
			"requestFullScreen": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#111111"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
